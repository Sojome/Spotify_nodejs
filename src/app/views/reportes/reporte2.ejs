<!DOCTYPE html>
<html html lang="es">
  <head>
    <meta charset="utf-8" />
    <title>Reportes</title>
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css"
      integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <%- include('../home/header.ejs') %>

    <div class="row">
      <div class="col-md-6 mt-4 text-center">
        <table class="table table-striped table-bordered">
          <tr>
            <th>Nombre de usuario</th>
            <th>Ultimo ingreso</th>
            <th>Cantidad de ingresos en total</th>
          </tr>
          <% for (var i = 0; i < reporte2.length; i ++) { %>
          <tr>
            <td><%- reporte2[i].Nombre_de_usuario %></td>
            <td><%- reporte2[i].Ultimo_ingreso %></td>
            <td><%- reporte2[i].Cantidad_de_ingresos_en_total %></td>
          </tr>
          <% } %>
        </table>
      </div>
    </div>
  </body>
</html>
