<!DOCTYPE html>
<html html lang="es">
  <head>
    <meta charset="utf-8" />
    <title>Reportes</title>
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
      integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk"
      crossorigin="anonymous"
    />
  </head>
  <body>
    <%- include('../home/header.ejs') %>

    <div class="row">
      <div class="col-md-6 mt-4 text-center">
        <table class="table table-striped table-bordered">
          <tr>
            <th>Titulo</th>
            <th>Artista</th>
            <th>Album</th>
            <th>Duracion</th>
            <th>Año de creación</th>
          </tr>
          <% for (var i = 0; i < reporte3.length; i ++) { %>
          <tr>
            <td><%- reporte3[i].Titulo %></td>
            <td><%- reporte3[i].Artista %></td>
            <td><%- reporte3[i].Album %></td>
            <td><%- reporte3[i].Duracion %></td>
            <td><%- reporte3[i].Año_de_creacion %></td>
          </tr>
          <% } %>
        </table>
      </div>
    </div>
  </body>
</html>
